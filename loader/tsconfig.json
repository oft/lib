{
  "include": ["./src/*.ts"],
    "compilerOptions": {
      "outDir": "./dist/",
      "noImplicitAny": true,
      "module": "es6",
      "target": "es2015", // 编译后
      //"jsx": "react",
      "sourceMap": true,
      
      "allowJs": true,
      "moduleResolution":"Node", // Classic  --module AMD | System | ES2015时的默认值为Classic，
      "types":[
        "systemjs"
      ]
    }
}